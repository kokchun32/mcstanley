

  <h1>Create New User</h1>
  <%= form_for :user, url: '/users' do |f| %>
    Username: <%= f.text_field :username %>
    <br>
    <br>
    Password: <%= f.password_field :password %>
    <br>
    <br>
    Role: <%= f.select :role, ['User','Admin'] %>
    <br>
    <br>
    Uplaod your photo: <%= f.file_field :image %>
    <br>
    <br>
  <%= f.submit "Submit" %>
  <% end %>

